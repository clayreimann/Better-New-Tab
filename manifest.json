{
  "name": "Better New Tab",
  "description": "Override the new tab page with a useful page",
  "version": "1.0",
  "incognito": "split",
  "chrome_url_overrides": {
    "newtab": "tab-page.html"
  },
  "background": {
    "scripts": ["js/background.js"]
  },
  "permissions": [
    "geolocation",
    "storage",
    "notifications"
  ],
  "manifest_version": 3,
  "icons": {
    "32": "img/cloud-laptop-32.png",
    "48": "img/cloud-laptop-48.png",
    "128": "img/cloud-laptop-128.png"
  }
}
