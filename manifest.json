{
  "name": "Better New Tab",
  "description": "Override the new tab page with a useful page",
  "version": "0.3",
  "incognito": "split",
  "chrome_url_overrides": {
    "newtab": "tab-page.html"
  },
  "background": {
    "scripts": ["js/background.js"]
  },
  "permissions": [
    "geolocation",
    "storage",
    "notifications"
  ],
  "manifest_version": 3
}
